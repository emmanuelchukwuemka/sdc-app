      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#16A34A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mathscrusader.surrogatedonorconnect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "adminPin": "6284",
      "referralBaseUrl": "https://sdc.example/ref",
      "eas": {
        "projectId": "40f30ee6-235c-4e83-be9b-527d38fff918"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-system-ui"
    ],
    "scheme": "surrogate",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "owner": "emmanuel-01"
  }
}